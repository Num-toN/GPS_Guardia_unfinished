{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "types": ["react", "react-native", "react-navigation"],
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
